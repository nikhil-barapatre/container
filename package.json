{
  "name": "container",
  "version": "0.1.0",
  "private": true,
  "scripts": {
   "dev": "NEXT_PRIVATE_LOCAL_WEBPACK=true next dev -p 3000",
   "build": "NEXT_PRIVATE_LOCAL_WEBPACK=true next build",
   "start": "NEXT_PRIVATE_LOCAL_WEBPACK=true next start -p 3000",
   
   "lint": "eslint",
   "update-dashboard": "rm -rf ./public/remotes/dashboard/_next/static && mkdir -p ./public/remotes/dashboard/_next/static && cp -r ../dashboard/.next/static/* ./public/remotes/dashboard/_next/static/",
   "update-remote2": "rm -rf ./public/remotes/remote2/_next/static && mkdir -p ./public/remotes/remote2/_next/static && cp -r ../remote2/.next/static/* ./public/remotes/remote2/_next/static/"
},
  "dependencies": {
    "@module-federation/nextjs-mf": "^8.8.38",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
